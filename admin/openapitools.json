{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.3.0",
    "generators": {
      "angular-api": {
        "generatorName": "typescript-angular",
        "output": "src/app/core/api",
        "additionalProperties": {
          "providedInRoot": true,
          "serviceSuffix": "ApiService",
          "apiModulePrefix": "App",
          "stringEnums": true
        }
      }
    }
  }
}
